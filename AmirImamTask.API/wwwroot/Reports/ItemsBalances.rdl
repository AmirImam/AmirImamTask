  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText> SELECT [i0].[ItemName], COALESCE(SUM([i].[Quantity]), 0.0E0) AS [Quantity]
      FROM [ItemStore] AS [i]
      INNER JOIN [Item] AS [i0] ON [i].[ItemId] = [i0].[Id]
      GROUP BY [i].[ItemId], [i0].[ItemName]</CommandText>
      </Query>
      <Fields>
        <Field Name="ItemName">
          <DataField>ItemName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Quantity">
          <DataField>Quantity</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
