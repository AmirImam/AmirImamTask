@page "/users"
<FormContainer>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>User name</th>
                <th>Email</th>
                <th>Phone number</th>
            </tr>
        </thead>

        <tbody>
            <Virtualize Context="person" Items=usersList>
                <tr>
                    <td>@person.PersonName</td>
                    <td>@person.Email</td>
                    <td>@person.PhoneNumber</td>
                </tr>
            </Virtualize>

        </tbody>
    </table>
</FormContainer>
